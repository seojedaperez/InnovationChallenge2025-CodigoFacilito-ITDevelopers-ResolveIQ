# Azure AI Foundry Agent Service
AZURE_AI_PROJECT_ENDPOINT=https://your-ai-foundry.services.ai.azure.com/api/projects/your-project
AZURE_AI_PROJECT_NAME=service-desk-agents

# Azure OpenAI Service
AZURE_OPENAI_ENDPOINT=https://your-openai.openai.azure.com/
AZURE_OPENAI_API_KEY=your-key-here
AZURE_OPENAI_GPT4O_DEPLOYMENT=gpt-4o
AZURE_OPENAI_O1_DEPLOYMENT=o1-preview
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-3-large

# Azure AI Search
AZURE_SEARCH_ENDPOINT=https://your-search.search.windows.net
AZURE_SEARCH_API_KEY=your-key-here
AZURE_SEARCH_INDEX_NAME=knowledge-base

# Azure Content Safety
AZURE_CONTENT_SAFETY_ENDPOINT=https://your-content-safety.cognitiveservices.azure.com/
AZURE_CONTENT_SAFETY_KEY=your-key-here

# Azure Cosmos DB
COSMOS_ENDPOINT=https://your-cosmos.documents.azure.com:443/
COSMOS_KEY=your-key-here
COSMOS_DATABASE_NAME=servicedesk

# Azure SQL Database
AZURE_SQL_CONNECTION_STRING=Driver={ODBC Driver 18 for SQL Server};Server=tcp:your-server.database.windows.net,1433;Database=servicedesk;Uid=your-user;Pwd=your-password;Encrypt=yes;TrustServerCertificate=no;

# Azure Cache for Redis
REDIS_HOST=your-redis.redis.cache.windows.net
REDIS_PORT=6380
REDIS_PASSWORD=your-password
REDIS_SSL=true

# Azure Service Bus
SERVICE_BUS_CONNECTION_STRING=Endpoint=sb://your-servicebus.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key
SERVICE_BUS_QUEUE_NAME=ticket-queue

# Azure Event Grid
EVENT_GRID_ENDPOINT=https://your-eventgrid.eventgrid.azure.net/api/events
EVENT_GRID_KEY=your-key-here

# Azure Storage Account
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=youraccount;AccountKey=your-key;EndpointSuffix=core.windows.net
AUDIT_LOG_CONTAINER=audit-logs

# Azure Key Vault
KEY_VAULT_URL=https://your-keyvault.vault.azure.net/

# Microsoft Graph API
GRAPH_CLIENT_ID=your-client-id
GRAPH_CLIENT_SECRET=your-client-secret
GRAPH_TENANT_ID=your-tenant-id

# Azure Speech Service
SPEECH_KEY=your-speech-key
SPEECH_REGION=eastus

# Application Insights
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your-key;IngestionEndpoint=https://eastus-8.in.applicationinsights.azure.com/;LiveEndpoint=https://eastus.livediagnostics.monitor.azure.com/;ApplicationId=your-app-id

# Bot Service
MICROSOFT_APP_ID=your-bot-app-id
MICROSOFT_APP_PASSWORD=your-bot-password

# Power Automate
POWER_AUTOMATE_BASE_URL=https://prod-00.eastus.logic.azure.com

# Application Settings
DEBUG=false
ENVIRONMENT=production
